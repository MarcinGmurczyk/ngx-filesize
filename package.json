{
  "name": "ngx-filesize",
  "description": "A filesize.js pipe for Angular",
  "version": "1.1.1",
  "license": "MIT",
  "main": "dist/index.umd.js",
  "unpkg": "dist/index.umd.min.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && npm run build:umd && npm run build:esm",
    "build:esm": "ngc -p tsconfig-aot.json",
    "build:umd": "webpack --mode production",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "@angular/core": ">=2.0.0 <7.0.0",
    "@angular/common": ">=2.0.0 <7.0.0"
  },
  "devDependencies": {
    "@angular/compiler": "^5.2.7",
    "@angular/compiler-cli": "^5.2.7",
    "@angular/core": "^5.2.7",
    "@ngtools/webpack": "^1.10.1",
    "@types/filesize": "^3.6.0",
    "awesome-typescript-loader": "^5.0.0-0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.6",
    "ts-loader": "^4.0.1",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.1.0",
    "webpack-angular-externals": "^1.0.2",
    "webpack-cli": "^2.0.10",
    "webpack-rxjs-externals": "^1.1.0",
    "zone.js": "^0.8.20"
  },
  "dependencies": {
    "filesize": "^3.6.0"
  },
  "repository": "github:amitdahan/ngx-filesize",
  "homepage": "https://github.com/amitdahan/ngx-filesize#readme",
  "bugs": "https://github.com/amitdahan/ngx-filesize/issues",
  "author": "Amit Dahan (amitdahan11@gmail.com)",
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "filesize",
    "filesizejs",
    "filesize.js",
    "pipe"
  ]
}
